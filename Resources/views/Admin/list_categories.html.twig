{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% for category in value %}
        <a href="{{ path( 'admin_kfi_cms_category_edit', { 'id' : category.id } ) }}">
            {{ category.title }}
        </a>
        {{ loop.last ? '' : ', ' }}
    {% endfor %}
{% endblock %}